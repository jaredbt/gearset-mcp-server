{
  "name": "@jaredbt/mcp-server-gearset",
  "version": "0.1.0",
  "description": "MCP server for Gearset CI/CD automation and DevOps workflows",
  "license": "MIT",
  "author": "Jared Thompson <jared@apollo.io>",
  "homepage": "https://github.com/jaredbt/gearset-mcp-server",
  "bugs": "https://github.com/jaredbt/gearset-mcp-server/issues",
  "type": "module",
  "bin": {
    "mcp-server-gearset": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "test": "jest --config=jest.config.cjs --coverage",
    "dev": "tsx index.ts",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write ."
  },
  "keywords": [
    "mcp",
    "gearset",
    "salesforce",
    "ci-cd",
    "devops",
    "model-context-protocol"
  ],
  "private": true,
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.1",
    "axios": "^1.7.7",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}